    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "http-status-codes": "^2.3.0",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "pdfreader": "^3.0.1"
